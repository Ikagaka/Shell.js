{
  "name": "ikagaka.shell.js",
  "version": "5.0.0",
  "description": "Ukagaka Shell Renderer for Web Browser",
  "license": "MIT",
  "url": "https://github.com/ikagaka/Shell.js",
  "keywords": [
    "nar",
    "ikagaka",
    "ukagaka"
  ],
  "scripts": {
    "setup": "npm install -g gulp-cli typings http-server",
    "init": "npm run update; npm run build",
    "update": "npm update; bower update; typings install",
    "tree": "tree -C -L 2 -I node_modules",
    "build": "gulp",
    "start": "http-server -s & gulp watch",
    "stop": "killall -- node */http-server -s",
    "clean": "rm -rf lib/* dist/* demo/test/*"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ikagaka/ikagaka/Shell.js.git"
  },
  "dependencies": {
    "events": "^1.1.0",
    "jquery": "3.*",
    "surfaces_txt2yaml": "legokichi/surfaces_txt2yaml#master"
  },
  "devDependencies": {
    "babel-core": "^6.11.4",
    "babel-preset-es2015": "^6.9.0",
    "empower": "^1.2.1",
    "gulp": "^3.9.1",
    "gulp-babel": "^6.1.2",
    "gulp-browserify": "^0.5.1",
    "gulp-espower": "^1.0.2",
    "gulp-typescript": "^2.13.6",
    "run-sequence": "^1.2.2",
    "typescript": "2.0.*"
  },
  "bugs": {
    "url": "https://github.com/Ikagaka/Shell.js/issues"
  },
  "typings": "./lib/index.d.ts",
  "main": "./lib/index.js",
  "author": "Ikagaka",
  "contributors": [
    "legokichi"
  ]
}
